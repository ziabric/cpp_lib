#include <iostream> // библеотека ввода/вывода
#include "rational.h" // подключение библеотеки проекта
int main()
{
    Rational rat1(2, 4);
    Rational rat2 = Rational(3,6);

    std::cout<<rat1<<'\n';
    std::cout<<rat2<<'\n';
    std::cout<<rat1+rat2<<'\n';
    std::cout<<rat1-rat2<<'\n';
    std::cout<<rat1*rat2<<'\n';
    std::cout<<rat1/rat2<<'\n';

    return 0;
}
